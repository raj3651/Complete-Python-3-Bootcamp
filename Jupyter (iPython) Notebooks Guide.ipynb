import software.amazon.awssdk.auth.credentials.AwsBasicCredentials;
import software.amazon.awssdk.auth.credentials.StaticCredentialsProvider;
import software.amazon.awssdk.regions.Region;
import software.amazon.awssdk.services.s3.S3Client;
import software.amazon.awssdk.services.s3.model.*;
import software.amazon.awssdk.core.ResponseInputStream;
import software.amazon.awssdk.services.s3.model.GetObjectRequest;

import java.io.FileOutputStream;
import java.io.FileNotFoundException;
import java.util.List;
import java.util.logging.Logger;

public class S3MultipartDownload {
    private static final Logger log = Logger.getLogger(S3MultipartDownload.class.getName());

    /**
     * Downloads a file from S3 using multipart download (range requests) via the S3 REST API.
     * This approach uses byte-range fetches to download the object in configurable parts.
     *
     * @param accessKey         AWS access key ID
     * @param secretKey         AWS secret access key
     * @param region            AWS region (e.g., "us-east-1")
     * @param bucketName        S3 bucket name
     * @param sourceFileName    S3 object key (file path in bucket)
     * @param destinationFileName Local file path to save the downloaded file
     * @throws Exception if any error occurs during download
     */
    public void multipartDownload(String accessKey, String secretKey, String region,
                                  String bucketName, String sourceFileName, String destinationFileName) throws Exception {
        try (S3Client s3Client = getS3Client(accessKey, secretKey, region)) {
            // Get the object metadata to retrieve file size
            HeadObjectResponse headObjectResponse = s3Client.headObject(HeadObjectRequest.builder()
                    .bucket(bucketName)
                    .key(sourceFileName)
                    .build());
            Long contentLength = headObjectResponse.contentLength();
            log.info("File size: {} bytes", contentLength);

            if (contentLength == 0) {
                throw new FileNotFoundException("File is empty");
            }

            long partSize = 10L * 1024 * 1024; // 10 MB parts; adjust as needed for performance
            try (FileOutputStream fos = new FileOutputStream(destinationFileName)) {
                // Download in sequential parts using range requests
                for (long start = 0; start < contentLength; start += partSize) {
                    long end = Math.min(start + partSize - 1, contentLength - 1);
                    GetObjectRequest rangeRequest = GetObjectRequest.builder()
                            .bucket(bucketName)
                            .key(sourceFileName)
                            .range(String.format("bytes=%d-%d", start, end))
                            .build();

                    try (ResponseInputStream<GetObjectResponse> responseInputStream = s3Client.getObject(rangeRequest)) {
                        byte[] buffer = new byte[8192]; // 8 KB buffer for reading
                        int bytesRead;
                        while ((bytesRead = responseInputStream.read(buffer)) != -1) {
                            fos.write(buffer, 0, bytesRead);
                        }
                    }
                    log.info("Downloaded part: bytes {}-{}", start, end);
                }
            }
            log.info("Download completed: {}", destinationFileName);
        } catch (Exception e) {
            log.severe("Error during multipart download: " + e.getMessage());
            throw new RuntimeException("Failed to download file from S3", e);
        }
    }

    /**
     * Creates an S3 client using AWS SDK v2.
     */
    private S3Client getS3Client(String accessKey, String secretKey, String region) {
        return S3Client.builder()
                .region(Region.of(region))
                .credentialsProvider(StaticCredentialsProvider.create(
                        AwsBasicCredentials.create(accessKey, secretKey)))
                .build();
    }

    // Example usage
    public static void main(String[] args) {
        S3MultipartDownload downloader = new S3MultipartDownload();
        try {
            downloader.multipartDownload(
                    "your-access-key",
                    "your-secret-key",
                    "us-east-1",
                    "your-bucket-name",
                    "path/to/your/file.txt",
                    "/local/path/to/save/file.txt"
            );
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}
